{
  "name": "shoppingweb",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "npm-run-all test server build",
    "test": "cross-env NODE_ENV=test mocha --require babel-core/register test/customer_test.js --timeout 3000 --exit",
    "server": "cross-env NODE_ENV=dev node lib/bin/www",
    "coverage": "cross-env NODE_ENV=test nyc mocha test/customer_test.js --timeout 3000 --exit",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./test --copy-files",
    "clean": "rimraf ./lib && mkdir lib",
    "postbuild": "npm run server",
    "prebuild": "npm-run-all test clean",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.418.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chai-things": "^0.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "http-errors": "~1.6.2",
    "httpserver": "^0.3.0",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^6.0.2",
    "mongodb": "^3.1.10",
    "mongoose": "^5.4.4",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
